{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "compile:sass": "sass ./src/styles/sass/main.scss:./src/styles/global.css .src/components/navbar.scss:src/components/navbar.module.css",
    "watch": "sass ./src/styles/sass/main.scss:./src/styles/global.css ./src/components/navbar.scss:./src/components/navbar.module.css -w"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "auth0-js": "^9.16.2",
    "gatsby": "^3.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "prettier": "2.2.1",
    "sass": "^1.34.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
